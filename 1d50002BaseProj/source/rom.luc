module rom (
    input address[5],  // reset 
    output outA[16], 
    output outB[16], 
    output alufn[6], 
    output outZVN[3], 
    output out[16] 
  ) {
  const A = {16b0, 16b01, 16b1, 16b0, 16b01, 16b1111111111111111, 16b0, 16b0100000000000001, 16b0000000000000001,16b1111111111111111, 16b1111111111111111,16b0, 16b0};
  const B = {16b0, 16b0, 16b0, 16b1, 16b11, 16b1111111111111111, 16b1, 16b0000000000000011, 16b1111111111111111, 16b1111111111111000, 16b1111111111111111, 16b0, 16b0};
  const AL = {b000000, b000001, b011000, b011110, b010110, b100011, b011010, b000010, b000010, b000010, b000010, b110011, b110101}; 
  const ZVN = {b100, b000, b100, b000, b000, b000, b100, b011, b001, b011, b011, b100, b100}; 
  const O = {16b0, 16b01, 16b0, 16b1, 16b10, 16b1111111111111111, 16b0,16b1100000000000011,16b1111111111111111, 16b0000000000001000,16b0000000000000001, 16b0000000000000001,16b0000000000000000}; 
  
  always {
    outA = A[address]; 
    outB = B[address]; 
    alufn = AL[address]; 
    outZVN = ZVN[address]; 
    out = O[address];
  }
}
